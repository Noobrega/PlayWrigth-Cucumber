{
    "files.autoSave": "afterDelay",
    "editor.fontSize": 20,
    "code-runner.clearPreviousOutput": true,
    "code-runner.runInTerminal": true,
    "tabnine.experimentalAutoImports": true,
    "window.meuBarVisibility": "compact",
    "cucumberautocomplete.steps": "./e2e/Tests/Steps/**/*.js",
    "cucumberautocomplete.syncfeatures": "./e2e/Tests/Features/**/*.feature",
    "cucumberautocomplete.syncDefinitions": true,
    "cucumberautocomplete.strictGherkinCompletion": true,
    "cucumberautocomplete.smartSnippets": true,
    "cucumberautocomplete.gherkinDefinitionPart": "Given|When|Then|And|But",
    "cucumberautocomplete.gherkinDefinitionPartRegex": "^(Given|When|Then|And|But)\\s+"
}